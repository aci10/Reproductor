<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.tarareapp_3.reproductor_tarareapp.MainActivity">

    <Button
        android:id="@+id/test_rep"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Testeo de reproductor" />

    <Button
        android:id="@+id/test_dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Testeo diagrama pianola" />

    <Button
        android:id="@+id/rep_detc_pitch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Deteccion pitch" />

    <Button
        android:id="@+id/tarsos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Prueba Tarsos"
        android:onClick="pruebaTarsosDSP"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="Frecuencia" />

</LinearLayout>
